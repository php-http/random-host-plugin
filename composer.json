{
    "name": "php-http/random-host-plugin",
    "description": "Client-side round robin load balancer plugin for HTTPlug",
    "license": "MIT",
    "keywords": [
        "http",
        "httplug",
        "plugin"
    ],
    "authors": [
        {
            "name": "Gabriel Ostroluck√Ω",
            "email": "gabriel.ostrolucky@gmail.com"
        }
    ],
    "homepage": "https://github.com/php-http/random-host-plugin",
    "require": {
        "php": "^7.4 || ^8.0",
        "php-http/client-common": "^2.0",
        "symfony/options-resolver": "^3.4 || ^4.4 || ^5.4 || ^6.0"
    },
    "require-dev": {
        "nyholm/psr7": "^1.0",
        "php-http/httplug": "^2.0",
        "php-http/mock-client": "^1.0",
        "ramsey/coding-standard": "^2.0",
        "symfony/phpunit-bridge": "^6.0"
    },
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Http\\Client\\Common\\Plugin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Http\\Client\\Common\\Plugin\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true
        }
    }
}
