{
    "name": "phphttp-plugin/set-random-host",
    "description": "Client-side round robin load balancer plugin for HTTPlug",
    "license": "MIT",
    "keywords": [
        "http",
        "httplug",
        "plugin"
    ],
    "authors": [
        {
            "name": "Gabriel OstroluckÃ½",
            "email": "gabriel.ostrolucky@gmail.com"
        }
    ],
    "homepage": "https://github.com/ostrolucky/httplug-plugin-use-random-host",
    "support": {
        "issues": "https://github.com/ostrolucky/httplug-plugin-use-random-host/issues",
        "source": "https://github.com/ostrolucky/httplug-plugin-use-random-host"
    },
    "require": {
        "php": ">=7.4",
        "php-http/client-common": ">=2.0"
    },
    "require-dev": {
        "nyholm/psr7": "^1.0",
        "php-http/httplug": "^2.0",
        "php-http/mock-client": "*",
        "ramsey/coding-standard": "^2.0",
        "symfony/phpunit-bridge": ">=6.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "PhpHttpPlugin\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true
        }
    }
}
